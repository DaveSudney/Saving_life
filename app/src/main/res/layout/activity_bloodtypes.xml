<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0F0F0"
    tools:context=".BloodtypesActivity">



    <RelativeLayout
        android:id="@+id/Orientacao"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/Avatar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            android:contentDescription="@string/app_name"/>

    </RelativeLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:orientation="vertical"
        android:paddingLeft="7dp"
        android:paddingRight="7dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Orientacao"

        tools:ignore="MissingConstraints">

        <LinearLayout
            android:foreground="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/grupo1"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/bg_branco"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="15dp"
            app:layout_constraintDimensionRatio="H,1:"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_margin="8dp"
                android:contentDescription="@string/app_name"
                android:padding="8dp"
                android:src="@drawable/img_23" />




        </LinearLayout>

        <LinearLayout
            android:foreground="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/grupo2"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/bg_branco"

            android:gravity="center"
            android:orientation="vertical"
            android:padding="15dp"
            app:layout_constraintDimensionRatio="H,1:"

            app:layout_constraintStart_toEndOf="@id/grupo1"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints">

            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_margin="8dp"
                android:contentDescription="@string/app_name"
                android:padding="8dp"
                android:src="@drawable/img_20" />




        </LinearLayout>

        <LinearLayout
            android:foreground="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/grupo4"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/bg_branco"

            android:gravity="center"
            android:orientation="vertical"
            android:padding="15dp"

            app:layout_constraintDimensionRatio="H,1:"
            app:layout_constraintEnd_toStartOf="@id/grupo2"
            app:layout_constraintTop_toBottomOf="@id/grupo1"
            tools:ignore="MissingConstraints">

            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_margin="8dp"
                android:contentDescription="@string/app_name"
                android:padding="8dp"
                android:src="@drawable/img_17" />




        </LinearLayout>

        <LinearLayout
            android:foreground="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/grupo5"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/bg_branco"

            android:gravity="center"
            android:orientation="vertical"
            android:padding="15dp"
            app:layout_constraintDimensionRatio="H,1:"
            app:layout_constraintLeft_toRightOf="@id/grupo6"

            app:layout_constraintTop_toTopOf="parent"

            tools:ignore="MissingConstraints">

            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_margin="8dp"
                android:contentDescription="@string/app_name"
                android:padding="8dp"
                android:src="@drawable/img_24" />




        </LinearLayout>

        <LinearLayout
            android:foreground="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/grupo6"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/bg_branco"

            android:gravity="center"
            android:orientation="vertical"
            android:padding="15dp"
            app:layout_constraintDimensionRatio="H,1:"
            app:layout_constraintLeft_toRightOf="@id/grupo2"
            app:layout_constraintTop_toTopOf="parent"

            tools:ignore="MissingConstraints">

            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_margin="8dp"
                android:contentDescription="@string/app_name"
                android:padding="8dp"
                android:src="@drawable/img_16" />



        </LinearLayout>

        <LinearLayout
            android:foreground="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/grupo7"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/bg_branco"

            android:gravity="center"
            android:orientation="vertical"
            android:padding="15dp"
            app:layout_constraintDimensionRatio="H,1:"
            app:layout_constraintLeft_toRightOf="@id/grupo3"
            app:layout_constraintTop_toBottomOf="@id/grupo5"
            tools:ignore="MissingConstraints">

            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_margin="8dp"
                android:contentDescription="@string/app_name"
                android:padding="8dp"
                android:src="@drawable/img_21" />



        </LinearLayout>

        <LinearLayout
            android:foreground="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/grupo8"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/bg_branco"

            android:gravity="center"
            android:orientation="vertical"
            android:padding="15dp"
            app:layout_constraintDimensionRatio="H,1:"
            app:layout_constraintStart_toEndOf="@id/grupo4"
            app:layout_constraintTop_toBottomOf="@id/grupo2"
            tools:ignore="MissingConstraints">

            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_margin="8dp"
                android:contentDescription="@string/app_name"
                android:padding="8dp"
                android:src="@drawable/img_19" />



        </LinearLayout>

        <LinearLayout
            android:foreground="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/grupo3"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/bg_branco"

            android:gravity="center"
            android:orientation="vertical"
            android:padding="15dp"
            app:layout_constraintDimensionRatio="H,1:"
            app:layout_constraintLeft_toRightOf="@id/grupo8"
            app:layout_constraintTop_toBottomOf="@id/grupo6"

            tools:ignore="MissingConstraints">

            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_margin="8dp"
                android:contentDescription="@string/app_name"
                android:padding="8dp"
                android:src="@drawable/img_18" />



        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>

